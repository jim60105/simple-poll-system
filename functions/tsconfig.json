{
  "compilerOptions": {
    "baseUrl": "..",
    "module": "esnext",
    "target": "esnext",
    "lib": ["esnext"],
    "alwaysStrict": true,
    "strict": true,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "types": ["@cloudflare/workers-types"],
    "paths": {
      "@/*": ["src/*"],
      "*": [
        "node_modules/*",
        "*"
      ],
    }
  }
}
